%e-yaml
---
Name: MinGW 32
Native: No
Maintainer: cheeseboy
Target-platform: Windows

# Some info about it
path:
make: make
cc: i686-w64-mingw32-gcc
cxx: i686-w64-mingw32-g++
defines: i686-w64-mingw32-cpp -dM -x c++ -E $blank
searchdirs: i686-w64-mingw32-gcc -E -x c++ -v $blank
searchdirs-start: "#include <...> search starts here:"
searchdirs-end: "End of search list."
resources: $exe
cppflags:
cxxflags: -I../Additional/Windows/include
cflags:
ldflags: -L../Additional/Windows/lib -static-libgcc -static-libstdc++
links:
windres: i686-w64-mingw32-windres

Build-Extension:
Run-output: $tempfile
Run-Program: wine $game
Run-Params:

