%e-yaml
---
Name: GNU GCC G++ 32
Native: Yes
Maintainer: cheeseboy
Target-platform: Linux

# Some info about it
path:
make: make
cc: gcc
cxx: g++
defines: cpp -dM -x c++ -E $blank
searchdirs: gcc -E -x c++ -v $blank
searchdirs-start: "#include <...> search starts here:"
searchdirs-end: "End of search list."
resources: $exe
cppflags:
cxxflags: -m32
cflags: -m32
ldflags: -m32
links:

Build-Extension:
Run-output: $tempfile
Run-Program: $game
Run-Params:

