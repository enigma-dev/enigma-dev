
struct PathPoint {
  x:int;
  y:int;
  speed:int;
}

table PathEditorMetadata {
  background_room_id:int;
  snap_x:int;
  snap_y:int;
}

table Path {
  name:string;
  id:int;

  editor_metadata:PathEditorMetadata;

  smooth:bool;
  closed:bool;
  precision:int;

  points:[PathPoint];
}

root_type Path;
