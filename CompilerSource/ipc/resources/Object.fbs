attribute "gmx";

table Event {
  id:int;
  code:string;
}

table MainEvent {
  id:int;
  events:[Event];
}

table Object {
  name:string;
  id:int;

  parent_name:string (gmx: "parentName");
  sprite_name:string (gmx: "spriteName");
  mask_name:string (gmx: "maskName");
  depth:int;
  solid:bool;
  visible:bool;
  persisent:bool;
  
  main_events:[MainEvent];
}

root_type Object;
